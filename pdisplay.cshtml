@model IEnumerable<test.Models.entity.v1>

@{
    ViewData["Title"] = "pdisplay";
}

<h1>pdisplay</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.pid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ptype)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tarea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.rent)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.pid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ptype)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tarea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rent)
            </td>
            <td>

                @Html.ActionLink("Edit", "Edit", new {  id=item.pid  }) |
              
            </td>
        </tr>
}
    </tbody>
</table>
